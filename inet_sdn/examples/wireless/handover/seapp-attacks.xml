<Unconditional>

<Attack>
	<start_time>100</start_time>
	<frequency>0.1</frequency>
	<action>
		<name>Create</name>
		<parameters>packetName:otherPacket:APP.type:0000</parameters>
	</action>
	<action>
		<name>Change</name>
		<parameters>packetName:otherPacket:field_name:sending.arrivalGate:value:"app_udp_sup$i[0]"</parameters>
	</action>
	
	<action>
		<name>Put</name>
		<parameters>packetName:otherPacket:nodes:4:direction:TX:throughWC:false:delay:0</parameters>
	</action>
</Attack>

</Unconditional>



