<?xml version="1.0"?>

<configuration>

	<Conditional>

		<Attack>
			<start_time>21</start_time>
			<node>17</node>
			<filter>[:NET.destAddress:==:192.168.0.11:]:[:attackInfo.fromGlobalFilter:==:1:]:AND</filter>

			<action>
				<name>Drop</name>
				<parameters>packetName:original:threshold:0</parameters>
			</action>
		</Attack>

	</Conditional>

	<Unconditional>

		<Attack>
			<start_time>20</start_time>
			<frequency>0.4</frequency>
			<var><name>1</name><value>1</value><type>NUMBER</type></var>
			<var><name>"myPacket"</name><value>myPacket</value><type>STRING</type></var>
			<var><name>26</name><value>26</value><type>NUMBER</type></var>
			<var><name>"app_tcp_inf$o[0]"</name><value>app_tcp_inf$o[0]</value><type>STRING</type></var>

			<action>
				<name>Create</name>
				<parameters>packetName:fakePacket:APP.type:1002</parameters>
			</action>

			<action>
				<name>Change</name>
				<parameters>packetName:fakePacket:field_name:APP.energyConsumption:value:1</parameters>
			</action>

			<action>
				<name>Change</name>
				<parameters>packetName:fakePacket:field_name:APP.name:value:"myPacket"</parameters>
			</action>

			<action>
				<name>Change</name>
				<parameters>packetName:fakePacket:field_name:controlInfo.connId:value:26</parameters>
			</action>

			<action>
				<name>Change</name>
				<parameters>packetName:fakePacket:field_name:controlInfo.userId:value:1</parameters>
			</action>

			<action>
				<name>Change</name>
				<parameters>packetName:fakePacket:field_name:sending.outputGate:value:"app_tcp_inf$o[0]"</parameters>
			</action>

			<action>
				<name>Put</name>
				<parameters>packetName:fakePacket:nodes:17:direction:TX:throughWC:false:delay:0</parameters>
			</action>
		</Attack>

	</Unconditional>

</configuration>